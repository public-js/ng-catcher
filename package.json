{
  "name": "ng-catcher",
  "version": "11.0.0-beta.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prepare": "husky install",
    "clean:b:demo": "rimraf ./dist/public-js/ng-catcher-demo",
    "clean:b:ng-catcher": "rimraf ./dist/public-js/ng-catcher",
    "clean:c:demo": "rimraf ./coverage/ng-catcher-demo",
    "clean:c:ng-catcher": "rimraf ./coverage/ng-catcher",
    "build:demo": "npm run clean:b:demo && ./node_modules/.bin/ng build --prod",
    "build:ng-catcher": "npm run clean:b:ng-catcher && ./node_modules/.bin/ng build --prod @public-js/ng-catcher",
    "lint:check": "eslint . --ext .js,.ts --cache --cache-strategy content",
    "lint:fix": "eslint . --ext .js,.ts --cache --cache-strategy content --fix",
    "cover:ng-catcher": "npm run clean:c:ng-catcher && ./node_modules/.bin/ng test @public-js/ng-catcher -- --code-coverage",
    "npm:dry:ng-catcher": "npm run build:ng-catcher && npm publish ./dist/public-js/ng-catcher --access public --dry-run",
    "npm:pub:ng-catcher": "npm run build:ng-catcher && npm publish ./dist/public-js/ng-catcher --access public"
  },
  "dependencies": {
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jasmine": "^3.6.9",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "core-js": "^3.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-decorator-position": "^2.2.23",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-rxjs": "^3.1.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-spec-reporter": "^0.0.32",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^11.2.4",
    "rimraf": "^3.0.0",
    "ts-node": "~8.3.0",
    "tsutils": "^3.21.0",
    "typescript": "~4.1.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "private": true
}
