{
  "name": "ng-catcher",
  "version": "11.0.0-beta.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean:demo": "rimraf dist/ng-catcher-demo",
    "build:demo": "ng build",
    "clean:ng-catcher": "rimraf dist/public-js/ng-catcher",
    "build:ng-catcher": "npm run clean:ng-catcher && ./node_modules/.bin/ng build --prod @public-js/ng-catcher",
    "cover:mocha:ng-catcher": "cd projects/public-js/ng-catcher && npm run cover:mocha",
    "clean:mocha:demo": "rimraf coverage/ng-catcher-demo && rimraf .nyc_output/ng-catcher-demo",
    "cover:mocha:demo": "npm run clean:mocha:demo && nyc npm run test:mocha:demo",
    "test:mocha:demo": "cross-env TS_NODE_PROJECT=\"tsconfig.mocha.json\" mocha",
    "npm:pub:ng-catcher": "npm run build:ng-catcher && npm publish dist/public-js/ng-catcher --access public",
    "npm:pub-test:ng-catcher": "npm run build:ng-catcher && npm publish dist/public-js/ng-catcher --access public --dry-run"
  },
  "dependencies": {
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "codelyzer": "^6.0.0",
    "core-js": "^3.10.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.2.1",
    "ng-packagr": "^11.2.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "private": true
}
