{
  "name": "ng-catcher",
  "version": "11.0.0-beta.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean:demo": "rimraf dist/ng-catcher-demo",
    "build:demo": "npm run clean:demo && ./node_modules/.bin/ng build",
    "clean:ng-catcher": "rimraf dist/public-js/ng-catcher",
    "build:ng-catcher": "npm run clean:ng-catcher && ./node_modules/.bin/ng build --prod @public-js/ng-catcher",
    "cover:jas:ng-catcher": "rimraf ./coverage && npm run test:jas:ng-catcher",
    "test:jas:ng-catcher": "./node_modules/.bin/ng test @public-js/ng-catcher -- --code-coverage",
    "npm:pub:ng-catcher": "npm run build:ng-catcher && npm publish dist/public-js/ng-catcher --access public",
    "npm:pub-test:ng-catcher": "npm run build:ng-catcher && npm publish dist/public-js/ng-catcher --access public --dry-run"
  },
  "dependencies": {
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/jasmine": "^3.6.9",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.10.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-spec-reporter": "^0.0.32",
    "ng-packagr": "^11.2.4",
    "rimraf": "^3.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "private": true
}
